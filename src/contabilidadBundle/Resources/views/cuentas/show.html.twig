{% extends '::base.html.twig' %}

{% block body -%}
    <h1>cuentas</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Cliente</th>
                <td>{{ entity.cliente }}</td>
            </tr>
            <tr>
                <th>Vercion</th>
                <td>{{ entity.vercion }}</td>
            </tr>
            <tr>
                <th>Totaldecuentas</th>
                <td>{{ entity.totalDeCuentas }}</td>
            </tr>
            <tr>
                <th>Mes</th>
                <td>{{ entity.mes }}</td>
            </tr>
            <tr>
                <th>Año</th>
                <td>{{ entity.año }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cuentas') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cuentas_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
