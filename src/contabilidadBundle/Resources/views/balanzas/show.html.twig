{% extends '::base.html.twig' %}

{% block body -%}
    <h1>balanzas</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ entity.version }}</td>
            </tr>
            <tr>
                <th>Rfc</th>
                <td>{{ entity.rfc }}</td>
            </tr>
            <tr>
                <th>Totalcuentas</th>
                <td>{{ entity.totalCuentas }}</td>
            </tr>
            <tr>
                <th>Mes</th>
                <td>{{ entity.mes }}</td>
            </tr>
            <tr>
                <th>Año</th>
                <td>{{ entity.año }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('balanzas') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('balanzas_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
